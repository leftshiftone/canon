               <block name="p92" class="psycho-row">
                    <selectable name="p93" class="psycho-col left">
                         <label>Anerkennung</label>
                    </selectable>
                    <selectable name="result" class="psycho-col">
                         <label>Sorgfalt</label>
                    </selectable>
               </block>
          </selectionItem>
          <selectionItem name="hamOrCheese">
